{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "node",
    "target": "es2015",
    "declaration": true,
    "outDir": "dist",
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "noEmit": true,
    "baseUrl": ".",
    "lib": [
      "es2015",
      "dom"
    ],
    "paths": {
      "cgol": ["src/index.ts"]
    },
    "rootDir": "src"
  },
  "include": [
    "src/**/*.ts",
    "src/index.ts"
  ],
  "files": [
    "src/index.ts",
    "tests/index.ts",
    "typings.d.ts"
  ]
}
